<?xml version="1.0" encoding="utf-8"?>
<s:ViewNavigatorApplication xmlns:fx="http://ns.adobe.com/mxml/2009"
							xmlns:s="library://ns.adobe.com/flex/spark"
							applicationDPI="160" firstView="views.RecordListView"
							splashScreenImage="@Embed('assets/screen_shot.png')" addedToStage="addedToStageHandler(event)">
	
	<fx:Script>
		<![CDATA[
			protected function addedToStageHandler(event:Event):void
			{
				// initializeイベントやcreationCompleteではタイミングが早くてthis.stageがnullになるのでaddedToStageイベントを使うのが確実
				this.stage.quality = StageQuality.MEDIUM;
				this.stage.frameRate = 24;
			}
		]]>
	</fx:Script>
	
	<fx:Declarations>
		<!-- 非ビジュアルエレメント (サービス、値オブジェクトなど) をここに配置 -->
	</fx:Declarations>
</s:ViewNavigatorApplication>
